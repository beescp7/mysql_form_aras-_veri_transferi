<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="650.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controller">
   <children>
      <TableView fx:id="table2" layoutX="240.0" layoutY="62.0" onMouseClicked="#event_tableclick" prefHeight="294.0" prefWidth="320.0">
        <columns>
          <TableColumn fx:id="tc_ad" prefWidth="75.0" text="AD" />
          <TableColumn fx:id="tc_soyad" prefWidth="75.0" text="Soyad" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="btn_goster" layoutX="431.0" layoutY="410.0" mnemonicParsing="false" onAction="#event_goster" text="Göster" />
      <Button fx:id="btn_detay" layoutX="240.0" layoutY="410.0" mnemonicParsing="false" onAction="#event_detay" text="Kullanıcı Detay" />
   </children>
</AnchorPane>
